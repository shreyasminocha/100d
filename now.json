{
    "name": "100d",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": { "distDir": "public" }
        },
        {
            "src": "api/*.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js"
        }
    ]
}
